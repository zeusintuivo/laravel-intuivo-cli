#!/usr/bin/env php
<?php
# @author Jesus Alcaraz <jesus@gammpartners.com>
#

$currentbranch = exec('git rev-parse --abbrev-ref HEAD');
echo "Pushing ".$currentbranch."\n" ;
$said = exec('git reset --hard origin/'.$currentbranch);
echo $said . "\n";
echo "Project Participation: \n";
include('projectparticipation');