#!/usr/bin/env bash

# set working directory as the scripts location during the execution of script 
cd "$(dirname "$0")"


# @author Jesus Alcaraz <jesus@gammpartners.com>
THISOUTPUT="$(php 'mysql_connectors')"
THISCONNECTOR=""
echo "mysql ${THISCONNECTOR}"
while read -r line; do
    #echo "... $line ..."
    THISCONNECTOR=$line
done <<< "$THISOUTPUT"
    echo "-- $THISCONNECTOR --"
#exit
mysql $THISCONNECTOR
